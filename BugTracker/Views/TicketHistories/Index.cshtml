@model IEnumerable<BugTracker.Models.TicketHistory>
@using GridMvc.Html

<script src="@Url.Content("~/Scripts/jquery-3.4.1.min.js")"></script>
<link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/gridmvc.min.js")" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<link href="@Url.Content("~/Content/Css/bootstrap.min.css")" rel="stylesheet" type="text/css" />


<h2>Ticket History</h2>

@Html.Grid(Model).Columns(columns =>
{
    columns.Add(t => t.Ticket.Title).Titled("Ticket Title");
    columns.Add(t => t.User.Email).Titled("UserName")
        .Sortable(true).Filterable(true);
    columns.Add(t => t.Property).Sortable(true).Filterable(true);
    columns.Add(t => t.OldValue);
    columns.Add(t => t.NewValue);
    columns.Add(t => t.Changed);
}).WithPaging(10)


<div>
    @Html.ActionLink("Back to List", "Index", "Tickets")
</div>
